{
	"name": "@phuture/sdk",
	"private": false,
	"repository": "Phuture-Finance/phuture-sdk",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "pnpm typechain && tsc",
		"fix": "pnpm lint --fix",
		"format": "prettier . -w",
		"lint": "eslint . --cache",
		"postinstall": "pnpm build",
		"preinstall": "npx only-allow pnpm",
		"prepare": "husky install",
		"test": "echo \"Error: no test specified\" && exit 0",
		"codegen": "graphql-codegen",
		"typechain": "typechain --target=ethers-v5 --out-dir=src/typechain 'src/abis/**/*.json' --always-generate-overloads",
		"types": "tsc --noEmit"
	},
	"devDependencies": {
		"@babel/core": "^7.20.5",
		"@ethersproject/abi": "^5.7.0",
		"@ethersproject/bytes": "^5.7.0",
		"@ethersproject/providers": "5.7.2",
		"@graphql-codegen/cli": "^2.15.0",
		"@graphql-codegen/client-preset": "^1.2.1",
		"@preconstruct/cli": "2.2.2",
		"@typechain/ethers-v5": "^10.1.1",
		"@types/debug": "^4.1.7",
		"@types/fs-extra": "9.0.13",
		"@types/node": "16.11.7",
		"@typescript-eslint/eslint-plugin": "5.42.1",
		"@typescript-eslint/parser": "5.42.1",
		"@zapper-fi/eslint-plugin": "1.4.0",
		"eslint": "8.27.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-import": "2.26.0",
		"eslint-plugin-prettier": "4.2.1",
		"eslint-plugin-unused-imports": "2.0.0",
		"husky": "8.0.1",
		"prettier": "2.7.1",
		"ts-node": "10.8.2",
		"typechain": "^8.1.1",
		"typedoc": "0.23.8",
		"typedoc-plugin-markdown": "3.11.6",
		"typescript": "4.7.4"
	},
	"engines": {
		"pnpm": ">=7"
	},
	"packageManager": "pnpm@7.14.0",
	"dependencies": {
		"@apollo/client": "^3.7.1",
		"@ethersproject/abstract-provider": "^5.7.0",
		"@graphql-typed-document-node/core": "^3.1.1",
		"axios": "^1.2.1",
		"axios-retry": "^3.3.1",
		"cross-fetch": "^3.1.5",
		"debug": "^4.3.4",
		"ethers": "^5.7.2",
		"graphql": "^16.6.0"
	}
