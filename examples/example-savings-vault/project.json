{
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "examples/example-savings-vault/src",
	"projectType": "application",
	"targets": {
		"build": {
			"executor": "@nrwl/node:webpack",
			"outputs": ["{options.outputPath}"],
			"options": {
				"outputPath": "dist/examples/example-savings-vault",
				"main": "examples/example-savings-vault/src/main.ts",
				"tsConfig": "examples/example-savings-vault/tsconfig.app.json"
			},
			"configurations": {
				"production": {
					"optimization": true,
					"extractLicenses": true,
					"inspect": false,
					"fileReplacements": [
						{
							"replace": "examples/example-savings-vault/src/environments/environment.ts",
							"with": "examples/example-savings-vault/src/environments/environment.prod.ts"
						}
					]
				}
			}
		},
		"exec": {
			"executor": "@nrwl/node:node",
			"options": {
				"buildTarget": "example-savings-vault:build"
			},
			"configurations": {
				"production": {
					"buildTarget": "example-savings-vault:build:production"
				}
			}
		},
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": ["examples/example-savings-vault/**/*.ts"]
			}
		},
		"test": {
			"executor": "@nrwl/jest:jest",
			"outputs": ["coverage/examples/example-savings-vault"],
			"options": {
				"jestConfig": "examples/example-savings-vault/jest.config.ts",
				"passWithNoTests": true
			}
		},
		"harvest": {}
	},
	"tags": []
}
