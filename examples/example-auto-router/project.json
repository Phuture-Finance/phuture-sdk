{
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "examples/example-auto-router/src",
	"projectType": "application",
	"targets": {
		"build": {
			"executor": "@nrwl/node:webpack",
			"outputs": ["{options.outputPath}"],
			"options": {
				"outputPath": "dist/examples/example-auto-router",
				"main": "examples/example-auto-router/src/main.ts",
				"tsConfig": "examples/example-auto-router/tsconfig.app.json"
			},
			"configurations": {
				"production": {
					"optimization": true,
					"extractLicenses": true,
					"inspect": false,
					"fileReplacements": [
						{
							"replace": "examples/example-auto-router/src/environments/environment.ts",
							"with": "examples/example-auto-router/src/environments/environment.prod.ts"
						}
					]
				}
			}
		},
		"exec": {
			"executor": "@nrwl/node:node",
			"options": {
				"buildTarget": "example-auto-router:build"
			},
			"configurations": {
				"production": {
					"buildTarget": "example-auto-router:build:production"
				}
			}
		},
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": ["examples/example-auto-router/**/*.ts"]
			}
		},
		"test": {
			"executor": "@nrwl/jest:jest",
			"outputs": ["coverage/examples/example-auto-router"],
			"options": {
				"jestConfig": "examples/example-auto-router/jest.config.ts",
				"passWithNoTests": true
			}
		}
	},
	"tags": []
}
