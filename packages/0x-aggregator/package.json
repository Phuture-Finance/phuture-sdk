{
  "name": "@phuture/0x-aggregator",
  "version": "0.1.0",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
	"dist"
  ],
  "scripts": {
	"test": "jest --coverage",
	"prepare": "npm run --silent build",
	"prepublishOnly": "npm lint",
	"build": "tsc",
	"clean": "rimraf dist",
	"lint": "xo",
	"lint:fix": "xo --fix"
  },
  "dependencies": {
	"axios": "^0.27.2",
	"ethers": "5.6.8"
  },
  "devDependencies": {
	"@phuture/types": "workspace:*",
	"@types/chai": "4.3.1",
	"@types/jest": "28.1.1",
	"chai": "4.3.6",
	"jest": "28.1.0",
	"ts-jest": "28.0.4",
	"typescript": "4.7.2",
	"xo": "0.49.0"
  },
  "jest": {
	"preset": "ts-jest",
	"testMatch": [
	  "**/test/*.test.ts"
	]
  },
  "xo": {
	"prettier": true,
	"rules": {
	  "import/extensions": 0
	},
	"ignores": [
	  "./test/**"
	]
  }
}
