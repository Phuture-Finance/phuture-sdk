      "index.ts"
    ]
  },
  "dependencies": {
    "@ethersproject/abstract-provider": "^5.7.0",
    "@phuture/0x-aggregator": "0.4.7",
    "@phuture/erc-20": "0.4.6",
    "@phuture/types": "0.4.6",
    "ethers": "5.7.2"
  }
}
