{
  "name": "@phuture/router",
  "version": "0.4.7",
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/Phuture-Finance/phuture-sdk",
    "directory": "packages/router"
  },
  "main": "dist/phuture-router.cjs.js",
  "module": "dist/phuture-router.esm.js",
