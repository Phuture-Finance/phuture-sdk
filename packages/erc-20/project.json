{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/erc-20/src",
  "projectType": "library",
  "targets": {
	"build": {
	  "executor": "@nrwl/js:tsc",
	  "outputs": [
		"{options.outputPath}"
	  ],
	  "options": {
		"outputPath": "dist/packages/erc-20",
		"main": "packages/erc-20/src/index.ts",
		"tsConfig": "packages/erc-20/tsconfig.lib.json",
		"assets": [
		  "packages/erc-20/*.md"
		]
	  }
	},
	"publish": {
	  "executor": "@nrwl/workspace:run-commands",
	  "options": {
		"command": "node tools/scripts/publish.mjs erc-20 {args.ver} {args.tag}"
	  },
	  "dependsOn": [
		{
		  "projects": "self",
		  "target": "build"
		}
	  ]
	},
	"generate": {
	  "executor": "@nrwl/workspace:run-commands",
	  "options": {
		"commands": [
		  {
			"command": "npx typechain --target=ethers-v5 --out-dir=packages/erc-20/src/types packages/erc-20/abis/**/*.json"
		  }
		]
	  }
	},
	"lint": {
	  "executor": "@nrwl/linter:eslint",
	  "outputs": [
		"{options.outputFile}"
	  ],
	  "options": {
		"lintFilePatterns": [
		  "packages/erc-20/**/*.ts"
		]
	  }
	},
	"test": {
	  "executor": "@nrwl/jest:jest",
	  "outputs": [
		"coverage/packages/erc-20"
	  ],
	  "options": {
		"jestConfig": "packages/erc-20/jest.config.ts",
		"passWithNoTests": true
	  }
	}
  },
  "tags": []
}
