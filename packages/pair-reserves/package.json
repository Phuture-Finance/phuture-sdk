{
  "name": "@phuture/pair-reserves",
  "version": "0.1.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "npm run --silent typechain && npm run --silent build",
    "prepublishOnly" : "npm lint && npm test",
    "build": "tsc",
    "typechain": "typechain --target=ethers-v5 --out-dir=src/types abis/**/*.json",
    "clean": "rimraf dist",
    "lint": "xo",
    "lint:fix": "xo --fix",
    "test": "ava"
  },
  "dependencies": {
    "ethers": "5.6.8"
  },
  "devDependencies": {
    "@phuture/types": "0.1.0",
    "@typechain/ethers-v5": "10.0.0",
    "ava": "4.2.0",
    "typechain": "8.0.0",
    "typescript": "4.7.2",
    "xo": "0.49.0"
  },
  "xo": {
    "prettier": true,
    "ignores": ["abis", "src/types/**/*.ts"],
    "rules": {
      "import/extensions": 0
    }
  }
}
