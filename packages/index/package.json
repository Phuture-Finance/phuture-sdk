		"entrypoints": [
			"index.ts"
		]
	},
	"dependencies": {
		"@ethersproject/abi": "^5.7.0",
		"@ethersproject/providers": "^5.7.2",
		"@phuture/account": "0.4.6",
		"@phuture/contract": "0.4.6",
		"@phuture/erc-20": "0.4.6",
		"@phuture/subgraph": "0.4.6",
		"@phuture/types": "0.4.6",
		"ethers": "5.7.2"
	}
}
