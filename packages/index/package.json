{
	"name": "@phuture/index",
	"version": "0.4.6",
	"type": "commonjs",
	"repository": {
		"type": "git",
		"url": "https://github.com/Phuture-Finance/phuture-sdk",
		"directory": "packages/index"
	},
	"main": "dist/phuture-index.cjs.js",
	"module": "dist/phuture-index.esm.js",
	"types": "dist/phuture-index.cjs.d.ts",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"module": "./dist/phuture-index.esm.js",
